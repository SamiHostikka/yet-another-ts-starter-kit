{
  "extends": [
    "tslint-microsoft-contrib",
    "tslint-immutable"
  ],
  "rules": {
    "no-var-keyword": true,
    "no-parameter-reassignment": true,
    "typedef": [true, "call-signature"],
    "readonly-keyword": true,
    "readonly-array": true,
    "no-let": true,
    "no-object-mutation": true,
    "no-this": true,
    "no-mixed-interface": true,
    "no-expression-statement": true,
    "missing-jsdoc": false
  }
}
